                function CountdownTime() {

                    if (countdownTimeWait == 0) {
                        $('#sbtnGetVerifyCode').removeAttr("disabled").val('获取动态密码');
                        countdownTimeWait = 60;
                    } else {
                        $('#sbtnGetVerifyCode').attr("disabled", true).val('重新发送(' + countdownTimeWait + ')');
